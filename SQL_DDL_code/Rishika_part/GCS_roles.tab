create table GCS_roles (
    gcs_rle_pk int not null auto_increment primary key, -- rle_id
    gcs_rle_uk1 varchar(50) not null, -- rle_name
    constraint UC_GCS_roles unique (gcs_rle_uk1), -- rle_name
    rle_discription varchar(100),
    rle_created_date date,
    rle_created_by varchar(40),
    rle_modified_date date,
    rle_modified_by varchar(40)
);